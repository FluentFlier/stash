    "@fastify/multipart": "^8.1.0",
    "@fastify/rate-limit": "^9.1.0",
    "@prisma/client": "^5.8.0",
    "@supabase/supabase-js": "^2.39.3",
    "bcrypt": "^5.1.1",
    "bull": "^4.12.0",
    "cheerio": "^1.0.0-rc.12",
    "date-fns": "^3.2.0",
    "dotenv": "^16.3.1",
    "fastify": "^4.25.2",
