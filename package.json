  "name": "stash-backend",
  "version": "1.0.0",
  "description": "Autonomous AI backend for Stash - Deep content analysis and agentic processing",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
